{
	"python.linting.pylintEnabled": true,
	"files.associations": {
		"iostream": "cpp",
		"ostream": "cpp",
		"random": "c",
		"vector": "cpp",
		"*.tcc": "cpp",
		"stdio.h": "c"
	},
	"python.formatting.provider": "black",
	"python.linting.pylintArgs": [
		"--disable=C0111"
	],
	"editor.formatOnSave": true,
	"files.eol": "\n",
	"editor.tabSize": 4,
	"python.analysis.typeCheckingMode": "basic"
}